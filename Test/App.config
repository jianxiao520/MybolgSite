<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="spring">
      <section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core" />
      <section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core" />
    </sectionGroup>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <appSettings>
    <add key="MemCachedServerList" value="127.0.0.1:6379" />
  </appSettings>

  <log4net>
    <!-- Define some output appenders -->
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="jianxiao.txt"/>
      <!--写到dll程序集所在的文件夹中了 名字就叫test.txt""-->
      <appendToFile value="true"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="1024KB"/>
      <rollingStyle value="Size"/>
      <!--//在这里的文件名是静态的，以后要做成动态的。-->
      <staticLogFileName value="true"/>
      <!--//日志的格式模板。  -5level表示日志的级别。-->
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline"/>
      </layout>
    </appender>
    <root>
      <!--//这里在调试的时候，就要改成DEBUG，如果是运行阶段，改成Error就可以了。就是这个级别都是随着配置走就可以了。-->
      <level value="DEBUG"/>
      <appender-ref ref="RollingLogFileAppender"/>
    </root>
  </log4net>



  <connectionStrings>
    <add name="new_userEntities" connectionString="metadata=res://*/Model1.csdl|res://*/Model1.ssdl|res://*/Model1.msl;provider=MySql.Data.MySqlClient;provider connection string=&quot;server=localhost;user id=root;password=jianxiao;persistsecurityinfo=True;database=new_user&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
  <spring>
    <context>
      <!--先后不能弄反顺序-->
      <!--<resource uri="config://spring/objects" />-->
      <resource uri="file://~/Config/dals.xml" />
      <resource uri="file://~/Config/services.xml" />
      <!--<resource uri="file://~/Config/Controllers.xml" />-->
      <!--<resource uri="file://~/Config/common.xml" />-->
    </context>

  </spring>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
